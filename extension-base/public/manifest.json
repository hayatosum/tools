{
    "manifest_version": 3,
    "name": "Extension",
    "version": "1.0.0",
    "action": {
        "default_popup": "index.html"
    },
    "background": {
        "service_worker": "background.js",
        "type": "module"
    },
    "host_permissions": ["https://www.googleapis.com/*", "https://www.gstatic.com/*"],
    "content_security_policy": {
        "extension_pages": "script-src 'self'; object-src 'self'"
    },
    "permissions": ["storage"]
}
